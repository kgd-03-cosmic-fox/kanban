ne":167,"column":0},"generated":{"line":1,"column":1860}},{"source":"src/App.vue","name":null,"original":{"line":167,"column":0},"generated":{"line":1,"column":1872}},{"source":"src/App.vue","name":null,"original":{"line":168,"column":0},"generated":{"line":1,"column":1885}},{"source":"src/App.vue","name":null,"original":{"line":168,"column":0},"generated":{"line":1,"column":1890}},{"source":"src/App.vue","name":null,"original":{"line":172,"column":0},"generated":{"line":1,"column":1916}},{"source":"src/App.vue","name":null,"original":{"line":172,"column":0},"generated":{"line":1,"column":1924}},{"source":"src/App.vue","name":null,"original":{"line":173,"column":0},"generated":{"line":1,"column":1935}},{"source":"src/App.vue","name":null,"original":{"line":173,"column":0},"generated":{"line":1,"column":1940}},{"source":"src/App.vue","name":null,"original":{"line":53,"column":0},"generated":{"line":1,"column":1956}},{"source":"src/App.vue","name":null,"original":{"line":53,"column":0},"generated":{"line":1,"column":1964}},{"source":"src/App.vue","name":null,"original":{"line":53,"column":0},"generated":{"line":1,"column":1972}}],"sources":{"src/App.vue":"<template>\r\n    <div>\r\n        <!-- form-register -->\r\n        <RegisterPage \r\n        v-if=\"currentPage === 'register-page' \" @changePage=\"changePage\">\r\n        </RegisterPage>\r\n        <!-- form login -->\r\n        <LoginPage \r\n        v-else-if=\"currentPage === 'login-page' \" \r\n        @changePage= \"changePage\" \r\n        @fetchData= \"fetchData\">\r\n        </LoginPage>\r\n        <div class=\"main-page\" v-else-if= \"currentPage === 'main-page' \">\r\n            <NavBar></NavBar>\r\n            <div class=\"header\">\r\n                <h2>Selamat Datang, {{user.name}}</h2>\r\n            </div>\r\n            <div class=\"cards\">\r\n                <div class=\"side-bar card-body col col-xl-3 border border-dark m-2 p-3\">\r\n                    <h5 class=\"card-title\">Card title</h5>\r\n                    <h6 class=\"card-subtitle mb-2 text-muted\">Card subtitle</h6>\r\n                    <p class=\"card-text\">Some quick example text to build on the card title and make up the bulk of the card's content.</p>\r\n                    <form @submit.prevent=\"addTodo\">\r\n                        <h3>Add New Todo</h3>\r\n                        <input v-model=\"newTodo.title\" type=\"text\" placeholder=\"Title\"><br>\r\n                        <input v-model=\"newTodo.description\" type=\"text\" placeholder=\"Description\"><br>\r\n                        <input v-model=\"newTodo.due_date\" type=\"date\"><br>           \r\n                        <button class=\"btn btn-secondary\">Add</button><br>\r\n                    </form>\r\n                </div>\r\n                <KanbanBoard\r\n                v-for=\"status in status\" \r\n                :key=\"status.id\" \r\n                :status=\"status\" \r\n                :todo=\"todoList\"\r\n                :colors=\"colors\" \r\n                class=\"col col-xl-2 border border-primary m-2 p-3 kanbanBoard\" \r\n                @fetchData=\"fetchData\"\r\n                >\r\n                </KanbanBoard>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport NavBar from './components/NavBar'\r\nimport LoginPage from './components/LoginPage'\r\nimport RegisterPage from './components/RegisterPage'\r\nimport KanbanBoard from './components/KanbanBoard'\r\n\r\nexport default {\r\n    name: \"App\",\r\n    components:{\r\n        NavBar, LoginPage, RegisterPage, KanbanBoard\r\n    },\r\n    data (){\r\n        return {\r\n            baseUrl:\"https://caneband-apps.herokuapp.com\",\r\n            currentPage: \"\",\r\n            user:{\r\n                name: \"\",\r\n                email: \"\",\r\n                password: \"\"\r\n            },\r\n            newTodo:{\r\n                title: \"\",\r\n                description: \"\",\r\n                due_date: \"\"\r\n            },\r\n            todoList: [],\r\n            organizations:[],\r\n            status:[\r\n                {\r\n                    id: 0,\r\n                    name: \"BackLog\"\r\n                },\r\n                {\r\n                    id: 1,\r\n                    name: \"To-Do\"\r\n                },\r\n                {\r\n                    id: 2,\r\n                    name: \"Doing\"\r\n                },\r\n                {\r\n                    id: 3,\r\n                    name: \"Completed\"\r\n                }\r\n            ],\r\n            colors:[\"primary\",\"warning\",\"danger\",\"success\"]\r\n        }\r\n    },\r\n    methods:{\r\n        changePage(page){\r\n            this.currentPage = page\r\n        },\r\n        addTodo(){\r\n            axios({\r\n                method:\"POST\",\r\n                url:`https://caneband-apps.herokuapp.com/todo`,\r\n                data:{\r\n                    title: this.newTodo.title,\r\n                    description: this.newTodo.description,\r\n                    due_date: this.newTodo.due_date\r\n                },\r\n                headers:{\r\n                    access_token: localStorage.getItem(\r\n                        \"access_token\"\r\n                    )\r\n                }\r\n            })\r\n            .then(_=>{\r\n                this.fetchData()\r\n            })\r\n            .catch(err=>{\r\n                console.log(err)\r\n            })\r\n        },\r\n        fetchData(){\r\n            axios({\r\n                method:\"GET\",\r\n                url: `https://caneband-apps.herokuapp.com/todo`,\r\n                headers:{\r\n                access_token: localStorage.getItem(\r\n                    `access_token`\r\n                    )\r\n                }\r\n            })\r\n            .then(response=>{\r\n                this.todoList = response.data.data\r\n                this.user.name = response.data.payload.name\r\n                // this.organizations = responsedata.payload.organization\r\n                // for(let a = 0; this.status.length; a++){\r\n                //  console.log(this.todoList.filter(todo=>todo.status==this.status[a].id))\r\n                // }\r\n            })\r\n            .catch(err=>{\r\n                console.log(err)\r\n            })\r\n        },\r\n        fetchAllOrganization(){\r\n            axios({\r\n                method:\"GET\",\r\n                url:`https://caneband-apps.herokuapp.com/organization`,\r\n                headers:{\r\n                    access_token: localStorage.getItem(\r\n                        \"access_token\"\r\n                    )\r\n                }\r\n            })\r\n            .then(response=>{\r\n                this.organizations = response.data.data\r\n            })\r\n            .catch(err=>{\r\n                console.log(err)\r\n            })\r\n        },\r\n        refreshPage(){\r\n            if(localStorage.getItem(`access_token`)){\r\n            this.currentPage = \"main-page\"\r\n            this.fetchData()\r\n            this.fetchAllOrganization()\r\n            }\r\n            else{\r\n                this.currentPage = \"login-page\"\r\n                this.fetchAllOrganization()\r\n            }\r\n        }\r\n    },\r\n    created(){\r\n       this.refreshPage()\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"},"lineCount":null}},"error":null,"hash":"170fd8d2bf7a9dbe7d795edc9296d405","cacheData":{"env":{}}}