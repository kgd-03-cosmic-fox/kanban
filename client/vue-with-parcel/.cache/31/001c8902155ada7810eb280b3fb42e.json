     <button class=\"mb-4 btn btn-primary\">Login</button>\r\n            <img class=\"logo-login\" src=\"../assets/img/caneband-logo.png\" alt=\"\" width=\"75px\">\r\n        </form>\r\n        <button class=\"btn btn-secondary\" @click=\"registerPage\">Register</button>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: \"LoginPage\",\r\n    data (){\r\n        return {\r\n            user:{\r\n                name: \"\",\r\n                email: \"\",\r\n                password: \"\"\r\n            }\r\n        }\r\n    },\r\n    methods:{\r\n         login(){\r\n            axios({\r\n                method:\"POST\",\r\n                url:`http://localhost:3000/login`,\r\n                data:{\r\n                    email: this.user.email,\r\n                    password: this.user.password,\r\n                }\r\n            })\r\n            .then(response=>{\r\n                this.user.name = response.data.payload.name\r\n                localStorage.setItem(\r\n                    `access_token`,\r\n                    response.data.token\r\n                    )\r\n                    this.$emit('changePage',\"main-page\")\r\n                    this.$emit('fetchData')\r\n           })\r\n            .catch(err=>{\r\n                console.log(err)\r\n            })\r\n            .finally(_=>{\r\n                this.user.name = \"\"\r\n                this.user.password= \"\"\r\n            })\r\n        },\r\n        registerPage(){\r\n            this.$emit('changePage','register-page')\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"},"lineCount":null}},"error":null,"hash":"8d90801a18ad0ea510a36c4df8ad6fc3","cacheData":{"env":{}}}